{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"name": "makeiteasy/slide",
	"title": "Slide",
	"parent": [
		"makeiteasy/slider"
	],
	"icon": "slides",
	"description": "Slide for makeiteasy slider",
	"apiVersion": 3,
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"category": "media",
	"attributes": {
		"slideWidth": {
			"type": "string"
		},
		"hideSlide": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"dimensions": {
			"aspectRatio": true,
			"minHeight": true
		},
		"shadow": true,
		"inserter": true
	}
}